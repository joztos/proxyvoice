{
  "version": 2,
  "builds": [
    { "src": "index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" }
  ],
  "env": {
    "DIALOGFLOW_PROJECT_ID": "acaiagent-esga",
    "DIALOGFLOW_SESSION_ID": "quickstart-session-id",
    "DIALOGFLOW_LANGUAGE_CODE": "es-ES"
  }
}
